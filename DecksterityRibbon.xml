<?xml version="1.0" encoding="UTF-8"?>
<customUI onLoad="Ribbon_Load" xmlns="http://schemas.microsoft.com/office/2006/01/customui">
	<ribbon>
		<tabs>
			<tab id="TabDecksterity" insertBeforeMso="TabHome" label="Decksterity" keytip="X">
				
				<group id="GroupDecksterityInsert" label="Insert">
					<gallery idMso="SlideNewGalleryInsert" size="large" keytip="isl" />
					<gallery idMso="TableInsertGallery" size="large" keytip="it" />
					<gallery idMso="ShapesInsertGallery" size="large" />
					<button idMso="IconInsertFromFile" size="large" keytip="ii" />
					<gallery idMso="ShapeFillColorPicker" size="large" keytip="sf" />
				</group>

				<group idMso="GroupFont" />

				<group id="GroupElements" label="Elements">
					<button id="HarveyBall0" getImage="getImage" label="0% Harvey Ball" showLabel="false" keytip="h0" onAction="HarveyBall0" />
					<button id="HarveyBall1" getImage="getImage" label="25% Harvey ball" showLabel="false" keytip="h1" onAction="HarveyBall1" />
					<button id="HarveyBall2" getImage="getImage" label="50% Harvey ball" showLabel="false" keytip="h2" onAction="HarveyBall2" />
					<button id="HarveyBall3" getImage="getImage" label="75% Harvey ball" showLabel="false" keytip="h3" onAction="HarveyBall3" />
					<button id="HarveyBall4" getImage="getImage" label="100% Harvey ball" showLabel="false" keytip="h4" onAction="HarveyBall4" />

					<separator id="ElementsSeparate1" />

					<button id="StoplightRed" getImage="getImage" label="Red stoplight" showLabel="false" keytip="slr" onAction="StoplightRed" />
					<button id="StoplightAmber" getImage="getImage" label="Amber stoplight" showLabel="false" keytip="sla" onAction="StoplightAmber" />
					<button id="StoplightGreen" getImage="getImage" label="Green stoplight" showLabel="false" keytip="slg" onAction="StoplightGreen" />

					<separator id="ElementsSeparate2" />

					<button id="IconCheck" getImage="getImage" label="Check" showLabel="false" keytip="ic" onAction="IconCheck" />
					<button id="IconPlus" getImage="getImage" label="Plus sign" showLabel="false" keytip="ip" onAction="IconPlus" />
					<button id="IconQuestion" getImage="getImage" label="Question mark" showLabel="false" keytip="iq" onAction="IconQuestion" />
					<button id="IconCross" getImage="getImage" label="Cross" showLabel="false" keytip="ix" onAction="IconCross" />
					<button id="IconMinus" getImage="getImage" label="Minus sign" showLabel="false" keytip="im" onAction="IconMinus" />
					<button id="IconEllipsis" getImage="getImage" label="Ellipsis" showLabel="false" keytip="ie" onAction="IconEllipsis" />

					<separator id="ElementsSeparate3" />

					<button id="ArrowNW" getImage="getImage" label="NW arrow" showLabel="false" keytip="anw" onAction="ArrowNW" />
					<button id="ArrowW" getImage="getImage" label="W arrow" showLabel="false" keytip="aww" onAction="ArrowW" />
					<button id="ArrowSW" getImage="getImage" label="SW arrow" showLabel="false" keytip="asw" onAction="ArrowSW" />

					<button id="ArrowN" getImage="getImage" label="N arrow" showLabel="false" keytip="ann" onAction="ArrowN" />
					<button id="Blank" getImage="getImage" enabled="false" />
					<button id="ArrowS" getImage="getImage" label="S arrow" showLabel="false" keytip="ass" onAction="ArrowS" />

					<button id="ArrowNE" getImage="getImage" label="NE arrow" showLabel="false" keytip="ane" onAction="ArrowNE" />
					<button id="ArrowE" getImage="getImage" label="E arrow" showLabel="false" keytip="aee" onAction="ArrowE" />
					<button id="ArrowSE" getImage="getImage" label="SE arrow" showLabel="false" keytip="ase" onAction="ArrowSE" />
				</group>

				<group id="GroupArrange" label="Arrange">
					<button id="AlignLeft" imageMso="ObjectsAlignLeft" label="Align left" showLabel="false" keytip="al" onAction="AlignLeft" />
					<button id="AlignBottom" imageMso="ObjectsAlignBottom" label="Align bottom" showLabel="false" keytip="ab" onAction="AlignBottom" />
					<button idMso="ObjectFlipHorizontal" showLabel="false" keytip="fh" />
					<button id="AlignCenter" imageMso="ObjectsAlignCenterHorizontal" label="Align center" showLabel="false" keytip="ac" onAction="AlignCenter" />
					<button id="AlignMiddle" imageMso="ObjectsAlignMiddleVertical" label="Align middle" showLabel="false" keytip="am" onAction="AlignMiddle" />
					<button idMso="ObjectFlipVertical" showLabel="false" keytip="fv"/>
					<button id="AlignRight" imageMso="ObjectsAlignRight" label="Align right" showLabel="false" keytip="ar" onAction="AlignRight" />
					<button id="AlignTop" imageMso="ObjectsAlignTop" label="Align top" showLabel="false" keytip="at" onAction="AlignTop" />
					<menu id="ObjectsResizeAndSpaceEvenly" label="Resize and space evenly" imageMso="AlignRelativeToPage" showLabel="false" keytip="rse" >
						<button id="ResizeAndSpaceEvenHorizontal" label="Resize and space horizontally (equal spacing)" onAction="ResizeAndSpaceEvenHorizontal" />
						<button id="ResizeAndSpaceEvenHorizontalPreserveFirst" label="Resize and space horizontally (preserve first)" onAction="ResizeAndSpaceEvenHorizontalPreserveFirst" />
						<button id="ResizeAndSpaceEvenHorizontalPreserveLast" label="Resize and space horizontally (preserve last)" onAction="ResizeAndSpaceEvenHorizontalPreserveLast" />
						<button id="ResizeAndSpaceEvenVertical" label="Resize and space vertically (equal spacing)" onAction="ResizeAndSpaceEvenVertical" />
						<button id="ResizeAndSpaceEvenVerticalPreserveFirst" label="Resize and space vertically (preserve first)" onAction="ResizeAndSpaceEvenVerticalPreserveFirst" />
						<button id="ResizeAndSpaceEvenVerticalPreserveLast" label="Resize and space vertically (preserve last)" onAction="ResizeAndSpaceEvenVerticalPreserveLast" />
					</menu>

					<separator id="ArrangeSeparate1"/>

					<button id="PrimaryAlignLeft" imageMso="ObjectsAlignLeft" label="Primary align left" showLabel="false" keytip="pal" onAction="PrimaryAlignLeft" />
					<button id="PrimaryAlignBottom" imageMso="ObjectsAlignBottom" label="Primary align bottom" showLabel="false" keytip="pab" onAction="PrimaryAlignBottom" />

					<button id="PrimaryAlignCenter" imageMso="ObjectsAlignCenterHorizontal" label="Primary align center" showLabel="false" keytip="pac" onAction="PrimaryAlignCenter" />
					<button id="PrimaryAlignMiddle" imageMso="ObjectsAlignMiddleVertical" label="Primary align middle" showLabel="false" keytip="pam" onAction="PrimaryAlignMiddle" />

					<button id="PrimaryAlignRight" imageMso="ObjectsAlignRight" label="Primary align right" showLabel="false" keytip="par" onAction="PrimaryAlignRight" />
					<button id="PrimaryAlignTop" imageMso="ObjectsAlignTop" label="Primary align top" showLabel="false" keytip="pat" onAction="PrimaryAlignTop" />

					<separator id="ArrangeSeparate2" />

					<button id="DistributeHorizontally" imageMso="AlignDistributeHorizontally" label="Distribute horizontally" showLabel="false" keytip="dh" onAction="DistributeHorizontally" />
					<button id="DistributeVertically" imageMso="AlignDistributeVertically" label="Distribute vertically" showLabel="false" keytip="dv" onAction="DistributeVertically" />

					<separator id="ArrangeSeparate3" />

					<button idMso="ObjectBringToFront" showLabel="false" keytip="btf" />
					<button idMso="ObjectBringForward" showLabel="false" keytip="bf" />
					<button idMso="ObjectRotateLeft90" showLabel="false" keytip="rl" />
					<button idMso="ObjectSendToBack" showLabel="false" keytip="stb" />
					<button idMso="ObjectSendBackward" showLabel="false" keytip="sb" />
					<button idMso="ObjectRotateRight90" showLabel="false" keytip="rr" />

					<separator id="ArrangeSeparate4" />

					<button id="ObjectsSameHeight" imageMso="RowHeight" label="Set same height" showLabel="false" keytip="ssh" onAction="SameHeight" />
					<button id="ObjectsSameWidth" imageMso="ColumnWidth" label="Set same width" showLabel="false" keytip="ssw" onAction="SameWidth" />
					<button id="ObjectsSwapPosition" imageMso="CircularReferences" label="Swap position" showLabel="false" keytip="psw" onAction="ObjectsSwapPositionCentered" />
				</group>
				
			</tab>
		</tabs>
	</ribbon>
</customUI>